{
  "explorer.experimental.fileNesting.enabled": true,
  "explorer.experimental.fileNesting.patterns": {
    "package.json": ".eslintrc.js, .pull-configs.js, dprint.json, jest.config.js, tsconfig.dist.json, tsconfig.json, web-test-runner.config.js"
  },
  "files.exclude": {
    ".gitattributes": true,
    ".gitignore": true,
    ".husky": true,
    ".npmrc": true,
    ".swc": true,
    ".vscode": true,
    "coverage": true
  },
  "todo-tree.tree.scanMode": "workspace only",
  "npm-scripts.showStartNotification": false
}
